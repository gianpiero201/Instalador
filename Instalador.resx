  <data name="htmlContentControl1.HtmlTemplate.Template" xml:space="preserve">
    <value>&lt;div class="content"&gt;
	&lt;img src="logo" class="image"&gt;
	&lt;div class="input-area"&gt;
		&lt;div&gt;
			&lt;span&gt;Login&lt;/span&gt;
			&lt;div class="input-div"&gt;
				&lt;input  class="input" name="LoginTextEdit"&gt;
			&lt;/div&gt;
		&lt;/div&gt;
		
		&lt;div&gt;
			&lt;span&gt;Senha&lt;/span&gt;
			&lt;div class="input-div"&gt;
				&lt;input class="input" name="PassTextEdit"&gt;
			&lt;/div&gt;
		&lt;/div&gt;	
	&lt;/div&gt;
	
	&lt;div name="LoginBtn" class="login-btn"&gt;&lt;/div&gt;
&lt;/div&gt;</value>
