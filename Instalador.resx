  <data name="htmlContentControl1.HtmlTemplate.Styles" xml:space="preserve">
    <value>.content {
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}

.input-area {
	width: 80%;
	height: 13em;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
}

.input-div {
	display: flex;
	justify-content: center;
	align-items: center;
	width: 100%;
	height: 3.5em;
	border-radius: 8px;
	border: 1px solid;
}

.input {
	height: 2.5em;
	width: 95%;
	padding: 0;
}

.image {
	width: 256px;
	height: 256px;
}

.login-btn {
	width: 90%;
	height: 16px;
	border: 1px solid;
}</value>
